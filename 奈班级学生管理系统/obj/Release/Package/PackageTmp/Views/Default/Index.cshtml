
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <script src="~/EChart/jquery-3.4.1.min.js"></script>
    <script src="~/jsCSSImg/Scripts/jquery-1.8.3.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <style>
    </style>
</head>
<body>
  
    <div id="waitPage" style="margin-top: 25%; margin-left: 40%;display:none">
        <div style="display:inline-block;float:left">
            <img style="height: 30px; width: 30px;" src="~/jsCSSImg/Lib/lhgdialog/skins/icons/loading.gif" />
        </div>
        <div style="display:inline-block;float:left;margin-top: 3px;font-size: 16px;">
            页面正在加载中...
        </div>
    </div>
    <iframe style="width: 100%; height: calc(100% - 43px); border: none; overflow: auto;" src="" id="iframe" onload="Page_wating()"></iframe>
    <script>
        //jq         iframe里写  onload事件
        function Page_wating() {//页面加载之前gif
            $("[id$='waitPage']").css("display", "none");
            $("[id$='iframe']").css("display", "block");
        }
    </script>



</body>
</html>
